<?xml version="1.0" encoding="UTF-8"?>
<web-app>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>InstrumentListServlet</servlet-name>
        <servlet-class>servlet.InstrumentListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InstrumentListServlet</servlet-name>
        <url-pattern>/listInstruments</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>EventBookingServlet</servlet-name>
        <servlet-class>servlet.EventBookingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EventBookingServlet</servlet-name>
        <url-pattern>/bookingEvent</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ListBookingEventsServlet</servlet-name>
        <servlet-class>servlet.ListBookingEventsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListBookingEventsServlet</servlet-name>
        <url-pattern>/listBookingEvents</url-pattern>
    </servlet-mapping>
</web-app>